CREATE TABLE `whatsappdelivery` (
  `whatsappdeliveryid` bigint NOT NULL AUTO_INCREMENT,
  `whatsappqueueapiid` bigint NOT NULL,
  `user_id` bigint DEFAULT NULL,
  `transactionid` varchar(64) DEFAULT NULL,
  `contactnumber` varchar(64) DEFAULT NULL,
  `message` varchar(5000) CHARACTER SET utf8mb4 COLLATE utf8mb4_unicode_ci DEFAULT NULL,
  `messagetype` varchar(64) DEFAULT NULL,
  `ip` varchar(64) DEFAULT NULL,
  `provider` varchar(64) CHARACTER SET utf8mb4 COLLATE utf8mb4_0900_ai_ci DEFAULT NULL,
  `wabanumber` varchar(13) NOT NULL COMMENT 'from_number of customer',
  `templateid` varchar(64) DEFAULT NULL,
  `entityid` varchar(64) DEFAULT NULL,
  `status` varchar(64) DEFAULT NULL,
  `campaignid` varchar(64) DEFAULT NULL,
  `submittime` datetime DEFAULT NULL,
  `creditrequired` int DEFAULT '0',
  `deductrequired` int DEFAULT NULL,
  `messageuuid` varchar(64) DEFAULT NULL,
  `messageid` varchar(256) DEFAULT NULL,
  `mediapath` varchar(500) DEFAULT NULL,
  `mediaid` varchar(500) DEFAULT NULL,
  `buttons` varchar(500) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci DEFAULT NULL,
  `dynamicUrl` varchar(500) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci DEFAULT NULL,
  `filename` varchar(500) CHARACTER SET utf8mb4 COLLATE utf8mb4_unicode_ci DEFAULT NULL COMMENT 'fileName incase docs',
  `dlrstatus` varchar(64) DEFAULT NULL,
  `senttime` datetime DEFAULT CURRENT_TIMESTAMP,
  `dlrfinalstatus` varchar(64) DEFAULT NULL,
  `dlrtime` datetime DEFAULT CURRENT_TIMESTAMP,
  `dlrduration` varchar(64) DEFAULT NULL,
  `error_title` varchar(500) CHARACTER SET utf8mb4 COLLATE utf8mb4_unicode_ci DEFAULT NULL,
  `error_details` text CHARACTER SET utf8mb4 COLLATE utf8mb4_unicode_ci,
  `campaignupdated` tinyint(1) NOT NULL DEFAULT '0',
  `scmq_id` bigint DEFAULT NULL,
  `parent_id` bigint DEFAULT NULL,
  `parent_user_type` tinyint DEFAULT NULL,
  `country_code` varchar(255) NOT NULL DEFAULT 'in',
  PRIMARY KEY (`whatsappdeliveryid`),
  KEY `idx_whatsappdelivery_messageid` (`messageid`),
  KEY `idx_whatsappdelivery_contactnum` (`contactnumber`,`submittime`,`user_id`) /*!80000 INVISIBLE */,
  KEY `idx_compositeusersubmitdeduct` (`user_id`,`deductrequired`,`submittime`) USING BTREE
) ENGINE=InnoDB AUTO_INCREMENT=2376005 DEFAULT CHARSET=latin1

