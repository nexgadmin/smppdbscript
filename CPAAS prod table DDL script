  `flow_response_id` bigint unsigned NOT NULL AUTO_INCREMENT,
  `flowintrection_id` bigint NOT NULL,
  `flow_id` bigint NOT NULL,
  `user_id` bigint NOT NULL,
  `replay_key` varchar(256) NOT NULL,
  `reply_response` text NOT NULL,
  `contactnumber` varchar(16) NOT NULL,
  `dialogtype` varchar(64) NOT NULL,
  `dialog_id` int NOT NULL,
  `createdon` datetime NOT NULL DEFAULT CURRENT_TIMESTAMP,
  `updatedon` datetime NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
  `status` varchar(256) NOT NULL,
  PRIMARY KEY (`flow_response_id`)
) ENGINE=InnoDB AUTO_INCREMENT=2038 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci

CREATE TABLE `flowmanager_interact` (
  `flowintrection_id` bigint NOT NULL AUTO_INCREMENT,
  `user_id` bigint NOT NULL,
  `flow_id` bigint NOT NULL,
  `waba_id` varchar(32) NOT NULL,
  `node_id` int NOT NULL,
  `waba_session_id` varchar(256) NOT NULL,
  `waba_message_id` varchar(256) NOT NULL,
  `contactnumber` varchar(16) NOT NULL,
  `start_dialog` varchar(32) NOT NULL,
  `current_dialog` varchar(32) NOT NULL,
  `next_dialog` text NOT NULL,
  `dialognname` varchar(256) NOT NULL,
  `dialogtype` varchar(64) NOT NULL,
  `template_id` varchar(256) DEFAULT NULL,
  `createdon` datetime NOT NULL DEFAULT CURRENT_TIMESTAMP,
  `updatedon` datetime NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
  `status` varchar(32) NOT NULL,
  `flow_status` varchar(256) NOT NULL COMMENT 'started, inprogress,completed',
  `flow_type` varchar(256) NOT NULL DEFAULT 'flowmanger',
  `flow_channel` varchar(256) NOT NULL DEFAULT 'whatsapp',
  PRIMARY KEY (`flowintrection_id`)
) ENGINE=InnoDB AUTO_INCREMENT=7440 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci
