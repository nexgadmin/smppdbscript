CREATE TABLE `smsdelivery` (
  `smsdeliveryid` bigint NOT NULL AUTO_INCREMENT,
  `smsqueueapiid` bigint NOT NULL,
  `user_id` bigint NOT NULL,
  `transactionid` varchar(64) NOT NULL,
  `contactnumber` varchar(64) DEFAULT NULL,
  `message` text CHARACTER SET utf8mb4 COLLATE utf8mb4_unicode_520_ci,
  `messagetype` varchar(64) DEFAULT NULL,
  `ip` varchar(64) DEFAULT NULL,
  `header` varchar(64) DEFAULT NULL,
  `templateid` varchar(64) DEFAULT NULL,
  `entityid` varchar(64) DEFAULT NULL,
  `status` varchar(64) DEFAULT NULL,
  `campaignid` varchar(64) DEFAULT NULL,
  `submittime` datetime DEFAULT NULL,
  `creditrequired` int DEFAULT '0',
  `dlrstatus` varchar(64) DEFAULT NULL,
  `senttime` datetime DEFAULT NULL,
  `dlrfinalstatus` varchar(64) DEFAULT NULL,
  `dlrtime` datetime DEFAULT NULL,
  `dlrduration` varchar(64) DEFAULT NULL,
  `messageid` varchar(64) NOT NULL,
  `dlrreason` varchar(1028) DEFAULT NULL,
  `dlrerrorcode` varchar(500) DEFAULT NULL,
  `smseid` varchar(64) DEFAULT NULL,
  `deltasubmitduration` varchar(64) DEFAULT NULL,
  `deltasentduration` varchar(64) DEFAULT NULL,
  `chargeType` varchar(64) DEFAULT NULL COMMENT 'submission/delivery',
  `delivery` decimal(4,3) DEFAULT NULL,
  `dlt` decimal(4,3) DEFAULT NULL,
  `parent_user_id` bigint DEFAULT NULL,
  `parent_id` bigint DEFAULT NULL,
  `parent_user_type` tinyint DEFAULT NULL,
  `servicetype` varchar(100) DEFAULT NULL,
  `shorturlId` varchar(255) DEFAULT NULL,
  `channel` varchar(255) NOT NULL DEFAULT 'nexg',
  `processType` varchar(64) NOT NULL DEFAULT 'simple',
  `scmq_id` bigint DEFAULT NULL,
  `flow_enbled` tinyint(1) DEFAULT NULL,
  `smscid` varchar(255) DEFAULT NULL,
  `smsc_id` varchar(255) DEFAULT NULL,
  PRIMARY KEY (`smsdeliveryid`),
  KEY `submittime` (`submittime`),
  KEY `key_transactionid` (`transactionid`) USING BTREE,
  KEY `key_messageid` (`messageid`) USING BTREE,
  KEY `dlrfinalstatus` (`dlrfinalstatus`,`dlrstatus`,`user_id`) USING BTREE /*!80000 INVISIBLE */
) ENGINE=InnoDB AUTO_INCREMENT=3458184 DEFAULT CHARSET=latin1

