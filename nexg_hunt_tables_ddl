 CREATE TABLE `SMSC_ID_MST_DEL` (
  `SMSC_ID` varchar(50) CHARACTER SET utf8mb4 COLLATE utf8mb4_0900_ai_ci DEFAULT NULL,
  `USER_NAME` varchar(50) DEFAULT NULL,
`SMSC_NAME` varchar(50) DEFAULT NULL,
`PLATFORM` varchar(10) DEFAULT NULL,
`INSERT_DATE` timestamp NULL DEFAULT NULL,
`CREATED_BY` varchar(50) CHARACTER SET utf8mb4 COLLATE utf8mb4_0900_ai_ci DEFAULT NULL,
`DELETE_DATE` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP,
`DELETED_BY` varchar(50) DEFAULT NULL,
  UNIQUE KEY `SMSC_ID_MST_INDEX` (`SMSC_ID`,`PLATFORM`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci

 CREATE TABLE `SUBSCRIPTION_MST` (
  `CUSTOMER_CODE` varchar(20) DEFAULT NULL,
  `USER_NAME` varchar(50) CHARACTER SET utf8mb4 COLLATE utf8mb4_0900_ai_ci NOT NULL,
  `SERVICE_NAME` varchar(50) DEFAULT NULL,
  `CHARGE_TYPE` varchar(20) DEFAULT NULL,
  `CUSTOMER_BASE_RATE` decimal(38,4) DEFAULT '0.0000',
  `DLT_RATE` decimal(38,4) DEFAULT '0.0000',
  `CHARGERATEUNIT` varchar(10) DEFAULT NULL,
  `SERVICE_TYPE` varchar(20) DEFAULT NULL,
  `PLATFORM` varchar(50) DEFAULT NULL,
  `EFFECTIVE_DATE` timestamp NULL DEFAULT CURRENT_TIMESTAMP,
  `STATUS` varchar(20) CHARACTER SET utf8mb4 COLLATE utf8mb4_0900_ai_ci DEFAULT 'ACTIVE',
  PRIMARY KEY (`USER_NAME`),
  KEY `SUBSCRIPTION_MST_INDEX` (`CUSTOMER_CODE`,`SERVICE_NAME`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci

CREATE TABLE `SUBSCRIPTION_MST_BKP` (
  `CUSTOMER_CODE` varchar(20) DEFAULT NULL,
  `USER_NAME` varchar(50) DEFAULT NULL,
  `SERVICE_NAME` varchar(50) DEFAULT NULL,
  `CHARGE_TYPE` varchar(20) DEFAULT NULL,
  `CUSTOMER_BASE_RATE` decimal(38,4) DEFAULT '0.0000',
  `DLT_RATE` decimal(38,4) DEFAULT '0.0000',
  `CHARGERATEUNIT` varchar(10) DEFAULT NULL,
  `SERVICE_TYPE` varchar(20) DEFAULT NULL,
  `PLATFORM` varchar(50) DEFAULT NULL,
  `EFFECTIVE_DATE` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP,
  `STATUS` varchar(20) DEFAULT 'PENDING',
  KEY `SUBSCRIPTION_MST_INDEX` (`CUSTOMER_CODE`,`SERVICE_NAME`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci

CREATE TABLE `TEMPLATE_LIST` (
  `ID` int NOT NULL AUTO_INCREMENT,
  `TEMPLATE_NAME` varchar(100) CHARACTER SET utf8mb4 COLLATE utf8mb4_0900_ai_ci DEFAULT NULL,
  `DISPLAY_NAME` varchar(50) DEFAULT NULL,
  `EMAIL_SUBJECT` varchar(100) CHARACTER SET utf8mb4 COLLATE utf8mb4_0900_ai_ci DEFAULT NULL,
  `MASSAGE_BODY` longblob,
  `USER_ID` int DEFAULT NULL,
  `USER_NAME` varchar(50) CHARACTER SET utf8mb4 COLLATE utf8mb4_0900_ai_ci DEFAULT NULL,
  `INSERT_DATE` timestamp NULL DEFAULT CURRENT_TIMESTAMP,
  PRIMARY KEY (`ID`),
  UNIQUE KEY `TEMPLATE_NAME` (`TEMPLATE_NAME`)
) ENGINE=InnoDB AUTO_INCREMENT=2 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci

CREATE TABLE `USER_MST` (
  `USER_ID` int NOT NULL AUTO_INCREMENT,
  `USER_NAME` varchar(30) CHARACTER SET utf8mb4 COLLATE utf8mb4_0900_ai_ci NOT NULL,
  `USER_PASSWORD` varchar(30) CHARACTER SET utf8mb4 COLLATE utf8mb4_0900_ai_ci NOT NULL,
  `USER_STATUS` varchar(10) CHARACTER SET utf8mb4 COLLATE utf8mb4_0900_ai_ci DEFAULT 'ACTIVE',
  `USER_TYPE` varchar(20) CHARACTER SET utf8mb4 COLLATE utf8mb4_0900_ai_ci DEFAULT NULL,
  `CUST_NAME` varchar(100) CHARACTER SET utf8mb4 COLLATE utf8mb4_0900_ai_ci DEFAULT NULL,
  `COMPANY_NAME` varchar(100) DEFAULT NULL,
  `ADDRESS` varchar(500) DEFAULT NULL,
  `ZONE` varchar(10) DEFAULT NULL,
  `STATE` varchar(50) CHARACTER SET utf8mb4 COLLATE utf8mb4_0900_ai_ci DEFAULT NULL,
  `CITY` varchar(50) DEFAULT NULL,
  `PINCODE` varchar(20) CHARACTER SET utf8mb4 COLLATE utf8mb4_0900_ai_ci DEFAULT NULL,
  `COUNTRY` varchar(50) DEFAULT NULL,
  `EMAIL_ID` varchar(100) CHARACTER SET utf8mb4 COLLATE utf8mb4_0900_ai_ci DEFAULT NULL,
  `MOBILE_NO` varchar(15) CHARACTER SET utf8mb4 COLLATE utf8mb4_0900_ai_ci DEFAULT NULL,
  `LAST_PWD_CHANGE` datetime NOT NULL DEFAULT '1970-01-01 00:00:00',
  `CREATION_DATE` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP,
  `DELETION_DATE` datetime NOT NULL DEFAULT '1970-01-01 00:00:00',
  `EXPIRY_DATE` datetime DEFAULT '2050-03-15 00:00:00',
  PRIMARY KEY (`USER_ID`),
  UNIQUE KEY `user_mst_index` (`USER_NAME`),
  FULLTEXT KEY `USER_NAME` (`USER_NAME`)
) ENGINE=InnoDB AUTO_INCREMENT=31 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci

 CREATE ALGORITHM=UNDEFINED DEFINER=`root`@`%` SQL SECURITY DEFINER VIEW `USER_NOT_FOUND` AS select `sms_cdrs`.`CRM_RATE_MST`.`CUSTOMER_CODE` AS `CUSTOMER_CODE`,trim(`sms_cdrs`.`CRM_RATE_MST`.`USER_NAME`) AS `USER_NAME`,`sms_cdrs`.`CRM_RATE_MST`.`SERVICE_NAME` AS `SERVICE_NAME`,`sms_cdrs`.`CRM_RATE_MST`.`CHARGE_TYPE` AS `CHARGE_TYPE`,`sms_cdrs`.`CRM_RATE_MST`.`CUSTOMER_BASE_RATE` AS `CUSTOMER_BASE_RATE`,`sms_cdrs`.`CRM_RATE_MST`.`DLT_RATE` AS `DLT_RATE`,`sms_cdrs`.`CRM_RATE_MST`.`CHARGERATEUNIT` AS `CHARGERATEUNIT`,trim(`sms_cdrs`.`CRM_RATE_MST`.`SERVICE_TYPE`) AS `SERVICE_TYPE` from `sms_cdrs`.`CRM_RATE_MST` where ((`sms_cdrs`.`CRM_RATE_MST`.`SERVICE_NAME` like '%SMS%') and (`sms_cdrs`.`CRM_RATE_MST`.`USER_NAME` <> 'NA') and (`sms_cdrs`.`CRM_RATE_MST`.`CUSTOMER_TYPE_CODE` <> 'DEMO') and (`sms_cdrs`.`CRM_RATE_MST`.`SERVICE_NAME` <> 'Demo Bulk SMS') and trim(`sms_cdrs`.`CRM_RATE_MST`.`USER_NAME`) in (select `CRM_RATE_MST`.`USER_NAME` from `CRM_RATE_MST`) is false)
