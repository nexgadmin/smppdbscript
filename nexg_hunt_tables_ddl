CREATE TABLE `BASIC_MST` (
  `NAME` varchar(50) NOT NULL,
  `TYPE` varchar(50) DEFAULT NULL,
  PRIMARY KEY (`NAME`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci

CREATE TABLE `CAMPAIGN_DATA` (
  `CAMPAIGN_ID` int NOT NULL AUTO_INCREMENT,
  `CAMPAIGN_NAME` varchar(100) CHARACTER SET utf8mb4 COLLATE utf8mb4_0900_ai_ci DEFAULT NULL,
  `USER_ID` int DEFAULT NULL,
  `USER_NAME` varchar(50) CHARACTER SET utf8mb4 COLLATE utf8mb4_0900_ai_ci DEFAULT NULL,
  `BASE_COUNT` int DEFAULT '0',
  `SUCCESS` int DEFAULT '0',
  `FAILED` int DEFAULT '0',
  `STATUS` varchar(20) DEFAULT NULL,
  `SCHEDULE` varchar(20) DEFAULT NULL,
  `CAMPAIGN_START_DATE` datetime DEFAULT NULL,
  `CAMPAIGN_END_DATE` datetime DEFAULT NULL,
  `INSERT_DATE` timestamp NULL DEFAULT CURRENT_TIMESTAMP,
  PRIMARY KEY (`CAMPAIGN_ID`),
  UNIQUE KEY `CAMPAIGN_NAME` (`CAMPAIGN_NAME`),
  KEY `USER_ID` (`USER_ID`,`INSERT_DATE`,`SUCCESS`,`SCHEDULE`,`CAMPAIGN_START_DATE`)
) ENGINE=InnoDB AUTO_INCREMENT=6 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci

CREATE TABLE `CITY_NAME` (
  `ID` bigint NOT NULL AUTO_INCREMENT,
  `STATE` varchar(50) CHARACTER SET utf8mb4 COLLATE utf8mb4_0900_ai_ci DEFAULT NULL,
  `DISTRICT` varchar(30) CHARACTER SET utf8mb4 COLLATE utf8mb4_0900_ai_ci DEFAULT NULL,
  `CITY` varchar(50) CHARACTER SET utf8mb4 COLLATE utf8mb4_0900_ai_ci DEFAULT NULL,
  `NAME_SHORT` varchar(5) CHARACTER SET utf8mb4 COLLATE utf8mb4_0900_ai_ci DEFAULT NULL,
  `ZONE` varchar(8) CHARACTER SET utf8mb4 COLLATE utf8mb4_0900_ai_ci DEFAULT NULL,
  PRIMARY KEY (`ID`),
  KEY `city` (`STATE`,`CITY`,`ZONE`)
) ENGINE=InnoDB AUTO_INCREMENT=580849 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci

CREATE ALGORITHM=UNDEFINED DEFINER=`root`@`%` SQL SECURITY DEFINER VIEW `CRM_NOT_FOUND` AS select `C`.`USER_NAME1` AS `USER_NAME1`,`C`.`PLATFORM` AS `PLATFORM`,`C`.`SUBMITION` AS `SUBMITION`,`C`.`DELIVERED` AS `DELIVERED`,`C`.`FAILED` AS `FAILED`,`D`.`CUSTOMER_TYPE_CODE` AS `CUSTOMER_TYPE_CODE`,`D`.`CUSTOMER_CODE` AS `CUSTOMER_CODE`,`D`.`CUSTOMER_NAME` AS `CUSTOMER_NAME`,`D`.`CUSTOMER_STATUS` AS `CUSTOMER_STATUS`,`D`.`CREATION_DATE` AS `CREATION_DATE`,`D`.`CUSTOMER_EMAIL_ID` AS `CUSTOMER_EMAIL_ID`,`D`.`SUBSCRIPTION_CODE` AS `SUBSCRIPTION_CODE`,`D`.`TRANSACTION_CODE` AS `TRANSACTION_CODE`,`D`.`SERVICE_NAME` AS `SERVICE_NAME`,`D`.`SERVICE_MODE` AS `SERVICE_MODE`,`D`.`CHARGE_TYPE` AS `CHARGE_TYPE`,`D`.`CUSTOMER_BASE_RATE` AS `CUSTOMER_BASE_RATE`,`D`.`DLT_RATE` AS `DLT_RATE`,`D`.`CHARGERATEUNIT` AS `CHARGERATEUNIT`,`D`.`SERVICE_TYPE` AS `SERVICE_TYPE`,`D`.`EMPLOYEE_NAME` AS `EMPLOYEE_NAME`,`D`.`EMPLOYEE_EMAILID` AS `EMPLOYEE_EMAILID`,`D`.`INSERT_DATE` AS `INSERT_DATE` from ((select `A`.`USER_NAME` AS `USER_NAME1`,`A`.`PLATFORM` AS `PLATFORM`,sum(`A`.`SUBMITION`) AS `SUBMITION`,sum(`A`.`DELIVERED`) AS `DELIVERED`,sum(`A`.`FAILED`) AS `FAILED` from `sms_cdrs`.`DAILY_SUMMARY` `A` where ((`A`.`SUBMITION` <> 0) and (`A`.`USER_NAME` <> 'NULL') and exists(select `B`.`USER_NAME` from `CRM_RATE_MST` `B` where (`B`.`USER_NAME` = `A`.`USER_NAME`)) is false) group by `A`.`USER_NAME`,`A`.`PLATFORM` order by `A`.`USER_NAME` desc) `C` left join (select `sms_cdrs`.`CRM_RATE_MST`.`CUSTOMER_CODE` AS `CUSTOMER_CODE`,`sms_cdrs`.`CRM_RATE_MST`.`USER_NAME` AS `USER_NAME`,`sms_cdrs`.`CRM_RATE_MST`.`CUSTOMER_NAME` AS `CUSTOMER_NAME`,`sms_cdrs`.`CRM_RATE_MST`.`CUSTOMER_EMAIL_ID` AS `CUSTOMER_EMAIL_ID`,`sms_cdrs`.`CRM_RATE_MST`.`CUSTOMER_STATUS` AS `CUSTOMER_STATUS`,`sms_cdrs`.`CRM_RATE_MST`.`CREATION_DATE` AS `CREATION_DATE`,`sms_cdrs`.`CRM_RATE_MST`.`CUSTOMER_TYPE_CODE` AS `CUSTOMER_TYPE_CODE`,`sms_cdrs`.`CRM_RATE_MST`.`SUBSCRIPTION_CODE` AS `SUBSCRIPTION_CODE`,`sms_cdrs`.`CRM_RATE_MST`.`TRANSACTION_CODE` AS `TRANSACTION_CODE`,`sms_cdrs`.`CRM_RATE_MST`.`SERVICE_NAME` AS `SERVICE_NAME`,`sms_cdrs`.`CRM_RATE_MST`.`SERVICE_MODE` AS `SERVICE_MODE`,`sms_cdrs`.`CRM_RATE_MST`.`CHARGE_TYPE` AS `CHARGE_TYPE`,`sms_cdrs`.`CRM_RATE_MST`.`CUSTOMER_BASE_RATE` AS `CUSTOMER_BASE_RATE`,`sms_cdrs`.`CRM_RATE_MST`.`DLT_RATE` AS `DLT_RATE`,`sms_cdrs`.`CRM_RATE_MST`.`CHARGERATEUNIT` AS `CHARGERATEUNIT`,`sms_cdrs`.`CRM_RATE_MST`.`SERVICE_TYPE` AS `SERVICE_TYPE`,`sms_cdrs`.`CRM_RATE_MST`.`EMPLOYEE_NAME` AS `EMPLOYEE_NAME`,`sms_cdrs`.`CRM_RATE_MST`.`EMPLOYEE_EMAILID` AS `EMPLOYEE_EMAILID`,`sms_cdrs`.`CRM_RATE_MST`.`INSERT_DATE` AS `INSERT_DATE` from `sms_cdrs`.`CRM_RATE_MST`) `D` on((`D`.`USER_NAME` = `C`.`USER_NAME1`))) where ((`C`.`DELIVERED` > 100) and (`D`.`CREATION_DATE` >= (curdate() - interval 90 day)))
character_set_client: utf8
collation_connection: utf8_general_ci

CREATE ALGORITHM=UNDEFINED DEFINER=`root`@`%` SQL SECURITY DEFINER VIEW `CRM_RATE_MST` AS select distinct `A`.`CUSTOMER_CODE` AS `CUSTOMER_CODE`,`B`.`USER_NAME` AS `USER_NAME`,upper(`A`.`CUSTOMER_NAME`) AS `CUSTOMER_NAME`,`A`.`EMAIL_ID` AS `CUSTOMER_EMAIL_ID`,`A`.`CUSTOMER_STATUS` AS `CUSTOMER_STATUS`,`A`.`CREATION_DATE` AS `CREATION_DATE`,`A`.`CUSTOMER_TYPE` AS `CUSTOMER_TYPE_CODE`,`B`.`SERVICE_NAME` AS `SERVICE_NAME`,`B`.`CHARGE_TYPE` AS `CHARGE_TYPE`,`B`.`CUSTOMER_BASE_RATE` AS `CUSTOMER_BASE_RATE`,`B`.`DLT_RATE` AS `DLT_RATE`,`B`.`CHARGERATEUNIT` AS `CHARGERATEUNIT`,`B`.`SERVICE_TYPE` AS `SERVICE_TYPE`,`A`.`SALES_EMP_NAME` AS `EMPLOYEE_NAME`,`A`.`EMPLOYEE_EMAILID` AS `EMPLOYEE_EMAILID`,`A`.`SUMMARY_EMAIL` AS `SUMMARY_EMAIL` from (`CUSTOMER_MST` `A` join `SUBSCRIPTION_MST` `B`) where ((`A`.`CUSTOMER_STATUS` = 'ACTIVE') and (`A`.`CUSTOMER_CODE` = `B`.`CUSTOMER_CODE`) and (`B`.`SERVICE_NAME` like '%SMS%'))
character_set_client: utf8
collation_connection: utf8_general_ci

 CREATE TABLE `CUSTOMER_BILLING` (
  `BILLING_MONTH` date DEFAULT NULL,
  `CUSTOMER_CODE` varchar(20) DEFAULT NULL,
  `USER_NAME` varchar(25) DEFAULT NULL,
  `CUSTOMER_NAME` varchar(100) DEFAULT NULL,
  `ACCOUNT_MANAGER` varchar(50) DEFAULT NULL,
  `CUSTOMER_STATUS` varchar(10) DEFAULT NULL,
  `CUSTOMER_TYPE_CODE` varchar(10) DEFAULT NULL,
  `SERVICE_NAME` varchar(50) DEFAULT NULL,
  `SERVICE_TYPE` varchar(20) DEFAULT NULL,
  `CHARGE_TYPE` varchar(20) DEFAULT NULL,
  `CUSTOMER_BASE_RATE` decimal(38,4) DEFAULT '0.0000',
  `DLT_RATE` decimal(38,4) DEFAULT '0.0000',
  `CHARGERATEUNIT` varchar(10) CHARACTER SET utf8mb4 COLLATE utf8mb4_0900_ai_ci DEFAULT NULL,
  `SUBMITED` int DEFAULT '0',
  `DELIVERED` int DEFAULT '0',
  `SMS_CHARGE` decimal(38,2) DEFAULT '0.00',
  `DLT_CHARGE` decimal(38,2) DEFAULT '0.00',
  `BILL_AMOUNT` decimal(38,2) DEFAULT '0.00',
  `GST` decimal(38,2) DEFAULT '0.00',
  `BILLED_AMOUNT` decimal(38,2) DEFAULT '0.00',
  `INSERT_DATE` timestamp NULL DEFAULT CURRENT_TIMESTAMP,
  KEY `BILLING_MONTH` (`BILLING_MONTH`,`CUSTOMER_CODE`,`USER_NAME`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci

CREATE TABLE `CUSTOMER_MST` (
  `ID` int NOT NULL AUTO_INCREMENT,
  `CUSTOMER_CODE` varchar(20) DEFAULT NULL,
  `CREATION_DATE` timestamp NULL DEFAULT NULL,
  `CAF_NO` varchar(13) DEFAULT NULL,
  `CUSTOMER_NAME` varchar(100) DEFAULT NULL,
  `CUSTOMER_TYPE` varchar(10) DEFAULT NULL,
  `REGISTRATION_NO` varchar(20) DEFAULT NULL,
  `PAN_GIR_NO` varchar(50) DEFAULT NULL,
  `TEL_MARKET_LIC_NO` varchar(20) DEFAULT NULL,
  `TEL_MARKET_LIC_VALIDITY` date DEFAULT NULL,
  `KEY_CONTACT_NAME` varchar(100) DEFAULT NULL,
  `DESIGNATION_CODE` varchar(50) DEFAULT NULL,
  `MOBILE_NO` varchar(10) DEFAULT NULL,
  `OTHER_MOBILE_NO` varchar(15) DEFAULT NULL,
  `EMAIL_ID` varchar(50) DEFAULT NULL,
  `CUSTOMER_STATUS` varchar(10) DEFAULT NULL,
  `CREATED_BY` varchar(50) DEFAULT NULL,
  `ENTITY_ID` varchar(20) DEFAULT NULL,
  `ADDRESS` varchar(250) DEFAULT NULL,
  `ZONE` varchar(10) DEFAULT NULL,
  `STATE` varchar(50) DEFAULT NULL,
  `CITY` varchar(50) DEFAULT NULL,
  `PINCODE` varchar(20) DEFAULT NULL,
  `COUNTRY` varchar(20) CHARACTER SET utf8mb4 COLLATE utf8mb4_0900_ai_ci DEFAULT 'INDIA',
  `SALES_EMP_NAME` varchar(50) DEFAULT NULL,
  `EMPLOYEE_EMAILID` varchar(50) DEFAULT NULL,
  `SALES_EMP_CHILD_NAME` varchar(50) DEFAULT NULL,
  `SALES_HEAD_NAME` varchar(50) CHARACTER SET utf8mb4 COLLATE utf8mb4_0900_ai_ci DEFAULT 'Vinod Mehlan',
  `SUMMARY_EMAIL` varchar(5) DEFAULT 'NO',
  PRIMARY KEY (`ID`),
  UNIQUE KEY `CUSTOMER_CODE_2` (`CUSTOMER_CODE`),
  FULLTEXT KEY `CUSTOMER_CODE` (`CUSTOMER_CODE`,`CUSTOMER_NAME`)
) ENGINE=InnoDB AUTO_INCREMENT=281 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci

 CREATE TABLE `CUSTOMER_OUTSTANDING` (
  `CUSTOMER_CODE` varchar(20) CHARACTER SET utf8mb4 COLLATE utf8mb4_0900_ai_ci NOT NULL,
