CREATE ALGORITHM=UNDEFINED DEFINER=`root`@`%` SQL SECURITY DEFINER VIEW `CUST_NOT_FOUND` AS select `sms_cdrs`.`CRM_RATE_MST`.`CUSTOMER_CODE` AS `CUSTOMER_CODE`,`sms_cdrs`.`CRM_RATE_MST`.`USER_NAME` AS `USER_NAME`,`sms_cdrs`.`CRM_RATE_MST`.`CUSTOMER_NAME` AS `CUSTOMER_NAME`,`sms_cdrs`.`CRM_RATE_MST`.`CUSTOMER_EMAIL_ID` AS `CUSTOMER_EMAIL_ID`,`sms_cdrs`.`CRM_RATE_MST`.`CUSTOMER_STATUS` AS `CUSTOMER_STATUS`,`sms_cdrs`.`CRM_RATE_MST`.`CREATION_DATE` AS `CREATION_DATE`,`sms_cdrs`.`CRM_RATE_MST`.`CUSTOMER_TYPE_CODE` AS `CUSTOMER_TYPE_CODE`,`sms_cdrs`.`CRM_RATE_MST`.`SUBSCRIPTION_CODE` AS `SUBSCRIPTION_CODE`,`sms_cdrs`.`CRM_RATE_MST`.`TRANSACTION_CODE` AS `TRANSACTION_CODE`,`sms_cdrs`.`CRM_RATE_MST`.`SERVICE_NAME` AS `SERVICE_NAME`,`sms_cdrs`.`CRM_RATE_MST`.`SERVICE_MODE` AS `SERVICE_MODE`,`sms_cdrs`.`CRM_RATE_MST`.`CHARGE_TYPE` AS `CHARGE_TYPE`,`sms_cdrs`.`CRM_RATE_MST`.`CUSTOMER_BASE_RATE` AS `CUSTOMER_BASE_RATE`,`sms_cdrs`.`CRM_RATE_MST`.`DLT_RATE` AS `DLT_RATE`,`sms_cdrs`.`CRM_RATE_MST`.`CHARGERATEUNIT` AS `CHARGERATEUNIT`,`sms_cdrs`.`CRM_RATE_MST`.`SERVICE_TYPE` AS `SERVICE_TYPE`,`sms_cdrs`.`CRM_RATE_MST`.`EMPLOYEE_NAME` AS `EMPLOYEE_NAME`,`sms_cdrs`.`CRM_RATE_MST`.`EMPLOYEE_EMAILID` AS `EMPLOYEE_EMAILID`,`sms_cdrs`.`CRM_RATE_MST`.`INSERT_DATE` AS `INSERT_DATE` from `sms_cdrs`.`CRM_RATE_MST` where ((`sms_cdrs`.`CRM_RATE_MST`.`SERVICE_NAME` like '%SMS%') and (`sms_cdrs`.`CRM_RATE_MST`.`USER_NAME` <> 'NA') and (`sms_cdrs`.`CRM_RATE_MST`.`CUSTOMER_TYPE_CODE` <> 'DEMO') and (`sms_cdrs`.`CRM_RATE_MST`.`SERVICE_NAME` <> 'Demo Bulk SMS') and `sms_cdrs`.`CRM_RATE_MST`.`CUSTOMER_CODE` in (select `CUSTOMER_MST`.`CUSTOMER_CODE` from `CUSTOMER_MST`) is false)
character_set_client: utf8
collation_connection: utf8_general_ci

CREATE TABLE `EMAIL_LOG` (
  `ID` bigint NOT NULL AUTO_INCREMENT,
  `CAMPAIGN_ID` int DEFAULT NULL,
  `CAMPAIGN_NAME` varchar(100) DEFAULT NULL,
  `TO_ID` varchar(4000) CHARACTER SET utf8mb4 COLLATE utf8mb4_0900_ai_ci DEFAULT NULL,
  `CC_ID` varchar(4000) CHARACTER SET utf8mb4 COLLATE utf8mb4_0900_ai_ci DEFAULT NULL,
  `BCC_ID` varchar(4000) CHARACTER SET utf8mb4 COLLATE utf8mb4_0900_ai_ci DEFAULT NULL,
  `SALUTATION` varchar(50) DEFAULT NULL,
  `ATTACHMENT` varchar(100) DEFAULT NULL,
  `TEMPLATE_ID` int DEFAULT NULL,
  `FROM_ID` varchar(50) DEFAULT NULL,
  `STATUS` varchar(50) DEFAULT NULL,
  `USER_ID` int DEFAULT NULL,
  `USER_NAME` varchar(50) CHARACTER SET utf8mb4 COLLATE utf8mb4_0900_ai_ci DEFAULT NULL,
  `ERROR_CODE` varchar(1000) DEFAULT NULL,
  `INSERT_DATE` timestamp NULL DEFAULT CURRENT_TIMESTAMP,
  PRIMARY KEY (`ID`),
  KEY `CAMPAIGN_ID` (`CAMPAIGN_ID`,`TEMPLATE_ID`,`STATUS`,`USER_ID`,`INSERT_DATE`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci

CREATE TABLE `LOAD_BASE` (
  `ID` int NOT NULL AUTO_INCREMENT,
  `CAMPAIGN_ID` int DEFAULT NULL,
  `CAMPAIGN_NAME` varchar(100) DEFAULT NULL,
  `FROM_ID` varchar(50) CHARACTER SET utf8mb4 COLLATE utf8mb4_0900_ai_ci NOT NULL DEFAULT '1',
  `TO_ID` varchar(4000) CHARACTER SET utf8mb4 COLLATE utf8mb4_0900_ai_ci DEFAULT NULL,
  `CC_ID` varchar(4000) CHARACTER SET utf8mb4 COLLATE utf8mb4_0900_ai_ci NOT NULL DEFAULT '1',
  `BCC_ID` varchar(4000) CHARACTER SET utf8mb4 COLLATE utf8mb4_0900_ai_ci NOT NULL DEFAULT '1',
  `SALUTATION` varchar(50) CHARACTER SET utf8mb4 COLLATE utf8mb4_0900_ai_ci NOT NULL DEFAULT 'Hi',
  `ATTACHMENT` varchar(100) CHARACTER SET utf8mb4 COLLATE utf8mb4_0900_ai_ci NOT NULL DEFAULT '1',
  `TEMPLATE_ID` int DEFAULT NULL,
  `USER_ID` int DEFAULT NULL,
  `USER_NAME` varchar(50) CHARACTER SET utf8mb4 COLLATE utf8mb4_0900_ai_ci DEFAULT NULL,
  `INSERT_DATE` timestamp NULL DEFAULT CURRENT_TIMESTAMP,
  `ISACTIVE` int DEFAULT '1',
  PRIMARY KEY (`ID`),
  KEY `CAMPAIGN_ID` (`CAMPAIGN_ID`,`FROM_ID`,`USER_ID`,`INSERT_DATE`,`ISACTIVE`)
) ENGINE=InnoDB AUTO_INCREMENT=3 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci

